@model List<Model.Common.Order_Entity>

@if (Model != null && Model.Count > 0)
{
    foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Car.BRAND
            </td>
            <td>
                @item.STARTDATE
            </td>
            <td>
                @item.ENDDATE
            </td>
            <td>
                @item.PICK_LOCATION
            </td>
            <td>
                @item.DROP_LOCATION
            </td>
            <td>
                @item.CONTACT_PERSON
            </td>
            <td>
                @item.ContactNo
            </td>
        </tr>
    }

}
else
{
    <tr>
        <td colspan="7" style="text-align:center"> No Record found</td>
    </tr>
}
